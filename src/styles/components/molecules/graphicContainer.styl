.graphic
  justify-self center
  display flex
  justify-content flex-end

.graphicContainer
  background-color white
  border-radius 25px
  margin 1.5vh
  height 95vh
  width 80%
  display grid
  grid-template-rows: 10% 90%
  margin-top: -99.5vh

.graphicContainer h1
  color #25284C
  padding-left: 30px

.graphicInf
  display grid
  grid-template-columns: 30% 70%

.graphicInf-vis
  display grid
  grid-template-rows 1fr 1fr
  justify-content center
  align-items center